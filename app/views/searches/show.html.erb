<div id="learner-list">
  <h1>Connect with these lifelong learners!</h1>
  <table class='table'>
    <tr>
      <th>Name</th>
      <th></th>
    </tr>
  <% @user_select.each do |user| %>
    <tr>
      <td><%= user.first_name %> <%= user.last_name %></td>
      <td><%= link_to 'Connect!', user_show_path(user), class: "user_link" %></td>
    </tr>
  <% end %>
  </table>
</div>
